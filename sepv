local plr = game.Players.LocalPlayer

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()
local w = library:CreateWindow("Heroes: Multiverse")
local b = w:CreateFolder("Main")
local m = w:CreateFolder("Misc")
local af = false

b:Toggle("SelfAuto Farm",function(v)
    af = v
end)

m:Button("woah?",function()
    setclipboard("yorrr")
end)

m:Button("Fuck off",function(value)
    for i,v in pairs(game:GetService("CoreGui"):GetChildren()) do
        if v:FindFirstChild("HiI'mSexyDon'tTouchMePls") then
            v:Destroy()
        end
    end
end)

spawn(function()
    while wait() do
        if af then
            pcall(function()
                game:GetService("ReplicatedStorage").Knit.Services.MeleeService.RE.Hit:FireServer({plr.Character}, 3)
            end)
        end
    end
end)
